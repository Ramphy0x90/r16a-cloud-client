{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"r16a-cloud_client": {
			"projectType": "application",
			"schematics": {},
			"root": "",
			"sourceRoot": "src",
			"prefix": "",
			"architect": {
				"build": {
					"builder": "@angular/build:application",
					"options": {
						"browser": "src/main.ts",
						"tsConfig": "tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "public"
							},
							{
								"glob": "**/*",
								"input": "src/assets",
								"output": "/assets"
							}
						],
						"styles": [
							"src/styles.css",
							"src/assets/styles/styles-light.css",
							"src/assets/styles/styles-dark.css",
							"node_modules/bootstrap-icons/font/bootstrap-icons.css"
						]
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2MB",
									"maximumError": "3MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2MB",
									"maximumError": "3MB"
								}
							],
							"outputHashing": "all"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "@angular/build:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "r16a-cloud_client:build:production"
						},
						"development": {
							"buildTarget": "r16a-cloud_client:build:development"
						}
					},
					"defaultConfiguration": "development"
				},
				"test": {
					"builder": "@angular/build:unit-test"
				}
			}
		}
	}
}
